    <!--
        !
        !   Geschäftsfall
        !
    -->
    <xsd:simpleType name="GeschaeftsfallArtType">
        <xsd:annotation>
            <xsd:documentation>Gruppierung der Geschäftsfälle als Kategorisierung für die Art des Kundenwunsches.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="50"/>
            <xsd:enumeration value="Bereitstellung">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Kuendigung">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Aenderung">
                <xsd:annotation>
                    <xsd:documentation/>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Entstoerung">
                <xsd:annotation>
                    <xsd:documentation>Entstörung</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Endkundenanbieterwechsel"/>
            <xsd:enumeration value="Diagnose">
                <xsd:annotation>
                    <xsd:documentation>Diagnose (Geschäftsfälle GET, SET)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="GeschaeftsfallMeldungType">
        <xsd:annotation>
            <xsd:documentation>Beschreibt den Geschäftsfall, in dessen Kontext die aktuelle Meldung versendet wird.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="NEU">
                <xsd:annotation>
                    <xsd:documentation>Bereitstellung (Bestellung neues Produkt)</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KUE-AG">
                <xsd:annotation>
                    <xsd:documentation>Kündigung durch den Auftraggeber</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="AEN-LMAE">
                <xsd:annotation>
                    <xsd:documentation>Änderung von Leistungsmerkmalen</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="LAE">
                <xsd:annotation>
                    <xsd:documentation>Änderung von Leistungen</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="EST">
                <xsd:annotation>
                    <xsd:documentation>Entstörung</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PV">
                <xsd:annotation>
                    <xsd:documentation>Providerwechsel</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="KUE-LE">
                <xsd:annotation>
                    <xsd:documentation>Kündigung durch den Leistungserbringer</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="GET">
                <xsd:annotation>
                    <xsd:documentation>Diagnose - GET</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="SET">
                <xsd:annotation>
                    <xsd:documentation>Diagnose - SET</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
